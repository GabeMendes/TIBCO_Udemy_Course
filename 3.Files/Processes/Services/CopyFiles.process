<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/3.Files/Resousers/Schemas/Services/CopyFilesRequest" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/3.Files/Resousers/Schemas/Services/CopyFilesResponse" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns2="http://www.tibco.com/schemas/0.ProjectZero/Resousers/Schemas/Commons/Response" xmlns:ns1="http://www.tibco.com/schemas/0.ProjectZero/Resousers/Schemas/Commons/Header">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resousers/Schemas/Services/CopyFilesRequest" schemaLocation="/Resousers/Schemas/Services/CopyFilesRequest.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resousers/Schemas/Services/CopyFilesResponse" schemaLocation="/Resousers/Schemas/Services/CopyFilesResponse.xsd"/>
    <pd:name>Processes/Services/CopyFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:CopyFilesRequest"/>
    <pd:startX>100</pd:startX>
    <pd:startY>65</pd:startY>
    <pd:returnBindings>
        <pfx2:CopyFilesResponse>
            <xsl:copy-of select="$Start/pfx:CopyFilesRequest/ns1:Header"/>
            <pfx2:Body>
                <ns2:Response>
                    <ns2:message>
                        <xsl:value-of select="&quot;file has been copied&quot;"/>
                    </ns2:message>
                    <ns2:responseStatus>
                        <xsl:value-of select="&quot;SUCCESS&quot;"/>
                    </ns2:responseStatus>
                </ns2:Response>
            </pfx2:Body>
        </pfx2:CopyFilesResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:CopyFilesResponse"/>
    <pd:endX>558</pd:endX>
    <pd:endY>63</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1754530895537</pd:targetNamespace>
    <pd:activity name="Copy File">
        <pd:type>com.tibco.plugin.file.FileCopyActivity</pd:type>
        <pd:resourceType>ae.activities.FileCopyActivity</pd:resourceType>
        <pd:x>347</pd:x>
        <pd:y>132</pd:y>
        <config>
            <overwrite>true</overwrite>
        </config>
        <pd:inputBindings>
            <ns:CopyActivityConfig>
                <fromFileName>
                    <xsl:value-of select="concat($Start/pfx:CopyFilesRequest/pfx:Body/pfx:source/pfx:sourceDirectoryPath,$Start/pfx:CopyFilesRequest/pfx:Body/pfx:source/pfx:sourceFileName)"/>
                </fromFileName>
                <toFileName>
                    <xsl:value-of select="concat($Start/pfx:CopyFilesRequest/pfx:Body/pfx:target/pfx:targetDirectoryPath,$Start/pfx:CopyFilesRequest/pfx:Body/pfx:target/pfx:targetFileName)"/>
                </toFileName>
            </ns:CopyActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Copy File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Copy File</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>