    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/2.ParseRenderXML/Resousers/Schemas/Models/Orders" schemaLocation="/Resousers/Schemas/Models/Orders.xsd"/>
